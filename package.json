{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A react project setup with high scalability, performance and best practice",
  "author": "Dehaat",
  "license": "ISC",
  "scripts": {
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "dev": "webpack server --progress --config src/webpack/webpack.dev.babel.js",
    "build": "webpack --config src/webpack/webpack.prod.babel.js --progress",
    "build:clean": "rimraf ./build",
    "test": "NODE_ENV=test jest",
    "test:coverage": "jest --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "lint": "npm run lint:js",
    "lint:js": "eslint --ext .jsx,.js,.ts,.tsx src/**/*",
    "lint:fix": "eslint --fix src/**/*",
    "prettify": "prettier --write .",
    "analyze": "webpack --config src/webpack/webpack.prod.babel.js --profile > stats",
    "analyze:clean": "rimraf stats",
    "storybook:static-files": "mkdir static-files && ln -s ../public static-files/public",
    "prestorybook": "npm run storybook:static-files && npm run test:generate-output",
    "storybook": "start-storybook -p 6006",
    "prebuild:storybook": "npm run storybook:static-files && npm run test:generate-output",
    "build:storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run lint:fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@types/dotenv-webpack": "^7.0.2",
    "chalk": "^4.1.1",
    "compression": "1.7.4",
    "history": "^5.0.0",
    "immer": "^9.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-optional-chaining": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.13.13",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.16",
    "@emotion/eslint-plugin": "^11.2.0",
    "@storybook/addon-a11y": "^6.4.12",
    "@storybook/addon-actions": "^6.4.12",
    "@storybook/addon-essentials": "^6.4.12",
    "@storybook/addon-jest": "^6.4.12",
    "@storybook/addon-links": "^6.4.12",
    "@storybook/react": "^6.4.12",
    "@storybook/testing-react": "^1.2.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "core-js": "^3.12.1",
    "coveralls": "^3.1.0",
    "css-loader": "^5.2.4",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "image-webpack-loader": "^7.0.1",
    "jest-cli": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^7.1.1",
    "terser-webpack-plugin": "^5.1.2",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.37.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
